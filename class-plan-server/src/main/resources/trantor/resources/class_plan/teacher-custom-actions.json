[{
  "key": "FilterAdmin", // action key, 模块内唯一
  "forModel": "class_plan_Teacher", // 所属模型全局 key, 组成方式: <moduleKey>_<modelKey>
  "extra": {
    "dataSource": "DataStore", // 待跳转目标视图数据来源类型，可选 DataStore | Static | Action | Empty
    "condition": "tname == tname", // 左侧的 name 为 forModel 里对应模型的字段名称，右侧的 name 为跳转到这个 action 时所传的参数名称
    "target": "class_plan_Teacher_FilterAdmin", // 跳转的目标视图，与 targetType 二选一， 在 view-manifest.json 里查找时去除 moduleKey 前缀
    "openViewType": "Self" // 打开视图的方式，默认为 Self ，其余可选项为 Blank | Dialog | Drawer | Columns
  }
}]